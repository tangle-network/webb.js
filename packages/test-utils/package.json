{
  "name": "@webb-tools/test-utils",
  "version": "0.1.4-72",
  "description": "Utilities to work with Webb in test environments",
  "author": "Webb Developers <drew@webb.tools>",
  "type": "module",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./index.js": {
      "types": "./index.d.ts",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./src/index.js": {
      "types": "./src/index.d.ts",
      "require": "./src/cjs/index.js",
      "default": "./src/index.js"
    }
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": "https://github.com/webb-tools/webb.js.git",
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@polkadot/api": "8.1.1",
    "@polkadot/keyring": "9.0.1",
    "@webb-tools/api": "0.1.4-72",
    "bignumber.js": "^9.0.0",
    "ethers": "5.5.1",
    "ganache": "7.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "chai": "^4.3.6"
  }
}
