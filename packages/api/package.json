{
  "name": "@webb-tools/api",
  "version": "0.1.4-75",
  "description": "Webb JS API",
  "type": "module",
  "author": "Webb Developers <drew@webb.tools>",
  "license": "Apache-2.0",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./index.js": {
      "types": "./index.d.ts",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./src/index.js": {
      "types": "./src/index.d.ts",
      "require": "./src/cjs/index.js",
      "default": "./src/index.js"
    },
    "./cjs/index.js": "./cjs/index.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": "https://github.com/webb-tools/webb.js.git",
  "bugs": {
    "url": "https://github.com/webb-tools/webb.js/issues"
  },
  "homepage": "https://github.com/webb-tools/webb.js",
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@polkadot/api": "8.6.2",
    "@polkadot/rpc-core": "8.6.2",
    "@webb-tools/api-derive": "0.1.4-75"
  }
}
