{
  "name": "@webb-tools/sdk-core",
  "version": "0.1.4-77",
  "description": "the core of webb tools sdk",
  "author": "Webb Developers <drew@webb.tools>",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./index.js": {
      "types": "./index.d.ts",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./src/index.js": {
      "types": "./src/index.d.ts",
      "require": "./src/cjs/index.js",
      "default": "./src/index.js"
    },
    "./proving": {
      "types": "./proving/index.d.ts",
      "require": "./cjs/proving/index.js",
      "default": "./proving/index.js"
    },
    "./cjs/proving": {
      "types": "./proving/index.d.ts",
      "require": "./cjs/proving/index.js",
      "default": "./cjs/proving/index.js"
    },
    "./proving/proving-manager.js": {
      "types": "./proving/proving-manager.d.ts",
      "require": "./cjs/proving/proving-manager.js",
      "default": "./proving/proving-manager.js"
    },
    "./cjs/proving/proving-manager.js": {
      "types": "./proving/proving-manager.d.ts",
      "require": "./cjs/proving/proving-manager.js",
      "default": "./cjs/proving/proving-manager.js"
    },
    "./proving/proving-manager-thread.js": {
      "types": "./proving/proving-manager-thread.d.ts",
      "require": "./cjs/proving/proving-manager-thread.js",
      "default": "./proving/proving-manager-thread.js"
    },
    "./cjs/proving/proving-manager-thread.js": {
      "types": "./proving/proving-manager-thread.d.ts",
      "require": "./cjs/proving/proving-manager-thread.js",
      "default": "./cjs/proving/proving-manager-thread.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": "https://github.com/webb-tools/webb.js.git",
  "bugs": {
    "url": "https://github.com/webb-tools/webb.js/issues"
  },
  "homepage": "https://github.com/webb-tools/webb.js",
  "dependencies": {
    "@polkadot/util": "9.3.1",
    "@types/events": "^3.0.0",
    "@webb-tools/api": "0.1.4-77",
    "@webb-tools/wasm-utils": "0.1.4-77",
    "bignumber.js": "^9.0.0",
    "events": "^3.2.0",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/lodash": "^4.14.161",
    "chai": "^4.3.6"
  }
}
