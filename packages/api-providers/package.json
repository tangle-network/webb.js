{
  "name": "@webb-tools/api-providers",
  "version": "0.1.4-64",
  "description": "dApp provider to work with Webb",
  "author": "Webb Developers <drew@webb.tools>",
  "type": "module",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./index.js": {
      "types": "./index.d.ts",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./src/index.js": {
      "types": "./src/index.d.ts",
      "require": "./src/cjs/index.js",
      "default": "./src/index.js"
    }
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": "https://github.com/webb-tools/webb.js.git",
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@polkadot/extension-dapp": "^0.35.0-beta.35",
    "@walletconnect/web3-provider": "^1.7.5",
    "@webb-tools/anchors": "0.1.15",
    "@webb-tools/contracts": "0.1.11",
    "@webb-tools/interfaces": "0.1.12",
    "@webb-tools/utils": "0.1.8",
    "bignumber.js": "^9.0.0",
    "snarkjs": "0.4.16"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "chai": "^4.3.6",
    "ganache": "7.1.0"
  }
}
